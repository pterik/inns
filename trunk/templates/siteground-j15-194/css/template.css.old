}

div#maincolumn_full {
	padding:0 0 20px;
	width:960px;
}
	
div.nopad {
	overflow: hidden;
}

div.nopad ul {
	clear: both;
}

td.middle_pad {
	width: 20px;
}

#footer {
	margin:0 auto;
	width:960px;
	color: #fff;
}

#footer p {
	text-align:center;
	line-height:19px;
	overflow:hidden;
	margin:0;
}

#footer a {
	color: #fff;
	font-weight:400;
	text-decoration: none;
}


#footer a:hover {
	text-decoration:underline;
}

a.footer123:link, a.footer123:visited {
	color: #363e4e;
	font-family: Tahoma, Arial, sans-serif;
	text-decoration: underline;
}

#f123 {
	text-align: right;
	width: 100%;
	margin: 0 auto;
	font-family: Tahoma, Arial, sans-serif;
}
.f123 {
	text-align: right;
	font-family: Tahoma, Arial, sans-serif;
